<?xml version="1.0" encoding="UTF-8"?>
<manifest>
<!--
    This branch is a main side branch and is configured to have all project
    specific rules. When updating this manifest file, please also update the
    project rule in google3 to ensure new projects get testing.
    See go/side-branch-testing for details.
-->
  <remote  name="aosp"
           fetch=".."
           review="https://android-review.googlesource.com/"/>
  <default revision="main"
           remote="aosp"
           sync-j="4"/>

  <superproject name="platform/superproject" remote="aosp" revision="rust-toolchain"/>
  <contactinfo bugurl="go/repo-bug" />

  <!-- Rust Toolchain Components -->
  <project path="toolchain/rustc" name="toolchain/rustc"/>
  <project path="toolchain/android_rust" name="toolchain/android_rust"/>
  <project path="toolchain/common" name="toolchain/common"/>
  <project path="toolchain/cargo-deny" name="toolchain/cargo-deny"/>
  <project path="toolchain/cargo-vet" name="toolchain/cargo-vet"/>

  <!-- LLVM Project Dependencies -->
  <project path="bionic" name="platform/bionic" />
  <project path="toolchain/binutils" name="toolchain/binutils" revision="llvm-r536225-release" />
  <project path="toolchain/llvm-project" name="toolchain/llvm-project" revision="llvm-r536225-release" />
  <project path="toolchain/llvm_android" name="toolchain/llvm_android" revision="llvm-r536225-release" />
  <project path="external/toolchain-utils" name="platform/external/toolchain-utils" revision="llvm-r536225-release" />
  <project path="external/zstd" name="platform/external/zstd" revision="llvm-r536225-release" />
  <project path="external/libedit" name="platform/external/libedit" revision="llvm-r536225-release" />
  <project path="external/swig" name="platform/external/swig" revision="llvm-r536225-release" />
  <project path="toolchain/xz" name="toolchain/xz" revision="llvm-r536225-release" />
  <project path="external/libxml2" name="platform/external/libxml2" revision="llvm-r536225-release" />
  <project path="external/libncurses" name="platform/external/ncurses" revision="llvm-r536225-release" />
  <project path="toolchain/prebuilts/ndk/releases" name="toolchain/prebuilts/ndk/releases" clone-depth="1" revision="llvm-r536225-release" />
  <project path="toolchain/prebuilts/sysroot/platform/riscv64-linux-android" name="toolchain/prebuilts/sysroot/platform/riscv64-linux-android" revision="llvm-r536225-release" />

  <!-- Other Toolchains -->
  <project path="prebuilts/clang/host/linux-x86" name="platform/prebuilts/clang/host/linux-x86" clone-depth="1"/>
  <project path="prebuilts/clang/host/darwin-x86" name="platform/prebuilts/clang/host/darwin-x86" groups="pdk,darwin" clone-depth="1" />
  <project path="prebuilts/clang/host/windows-x86" name="platform/prebuilts/clang/host/windows-x86" clone-depth="1"/>
  <project path="prebuilts/gcc/darwin-x86/host/headers" name="platform/prebuilts/gcc/darwin-x86/host/headers" groups="pdk,darwin" clone-depth="1" revision="llvm-r536225-release" />
  <project path="prebuilts/gcc/linux-x86/host/x86_64-linux-glibc2.17-4.8" name="platform/prebuilts/gcc/linux-x86/host/x86_64-linux-glibc2.17-4.8" clone-depth="1" revision="llvm-r536225-release" />
  <project path="prebuilts/gcc/linux-x86/host/x86_64-w64-mingw32-4.8" name="platform/prebuilts/gcc/linux-x86/host/x86_64-w64-mingw32-4.8" clone-depth="1" revision="llvm-r536225-release" />

  <!-- Tool prebuilts -->
  <project path="prebuilts/build-tools" name="platform/prebuilts/build-tools" clone-depth="1" revision="llvm-r536225-release" />
  <project path="prebuilts/cmake/darwin-x86" name="platform/prebuilts/cmake/darwin-x86" clone-depth="1" revision="llvm-r536225-release" />
  <project path="prebuilts/cmake/linux-x86" name="platform/prebuilts/cmake/linux-x86" clone-depth="1" revision="llvm-r536225-release" />
  <project path="prebuilts/cmake/windows-x86" name="platform/prebuilts/cmake/windows-x86" clone-depth="1"/>
  <project path="prebuilts/go/darwin-x86" name="platform/prebuilts/go/darwin-x86" groups="darwin,pdk,tradefed" clone-depth="1" revision="llvm-r536225-release" />
  <project path="prebuilts/go/linux-x86" name="platform/prebuilts/go/linux-x86" groups="linux,pdk,tradefed" clone-depth="1" revision="llvm-r536225-release" />
  <project path="prebuilts/ninja/darwin-x86" name="platform/prebuilts/ninja/darwin-x86" clone-depth="1"/>
  <project path="prebuilts/ninja/linux-x86" name="platform/prebuilts/ninja/linux-x86" clone-depth="1"/>
  <project path="prebuilts/ninja/windows-x86" name="platform/prebuilts/ninja/windows-x86" clone-depth="1"/>

  <!-- Python prebuilts -->
  <project path="prebuilts/python/linux-x86" name="platform/prebuilts/python/linux-x86" clone-depth="1" revision="llvm-r536225-release" />
  <project path="prebuilts/python/darwin-x86" name="platform/prebuilts/python/darwin-x86" clone-depth="1" revision="llvm-r536225-release" />
  <project path="prebuilts/python/windows-x86" name="platform/prebuilts/python/windows-x86" clone-depth="1" revision="llvm-r536225-release" />

  <!-- Our Prebuilts -->
  <project path="prebuilts/rust" name="platform/prebuilts/rust" clone-depth="1"/>

  <!-- Build System Components -->
  <project path="build/soong" name="platform/build/soong" >
    <linkfile src="root.bp" dest="Android.bp" />
    <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
  </project>
</manifest>

